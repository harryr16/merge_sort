def merge_sort(list_a, list_b):
    a = 0
    b = 0
    merge_list = []
    while a < len(list_a) and b < len(list_b):
        if list_a[a] > list_b[b]:
            merge_list.append(list_b[b])
            b += 1
        else:
            merge_list.append(list_a[a])
            a += 1
    retu        




        